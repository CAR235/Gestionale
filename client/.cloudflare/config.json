{
  "build": {
    "command": "npm install && npm run build",
    "output": "dist",
    "environment": {
      "NODE_VERSION": "18"
    }
  },
  "routes": [
    {
      "pattern": "/*",
      "zone_id": "${CF_ZONE_ID}",
      "custom_domain": true,
      "access": {
        "teamName": "${CF_TEAM_NAME}"
      }
    }
  ]
}